<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="Redirect unversioned paths to 10.0/ path">
                    <match url="(.*)" />
                    <conditions>
                        <add input="{URL}" pattern="(version-history|\d+\.\d+)(\/)?" negate="true" />
                    </conditions>
                    <action type="Redirect" url="10.0/{R:1}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
