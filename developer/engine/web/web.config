<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="Redirect legacy /content/ paths to corresponding subfolder." stopProcessing="true">
                    <match url="^content(\/)?(.*)" />
                    <action type="Redirect" url="2.0/{R:2}" />
                </rule>

                <rule name="Redirect unversioned paths to 12.0/ path">
                    <match url="^(.*)" />
                    <conditions>
                        <add input="{URL}" pattern="(1|2|10|11|12)\.0(\/)?" negate="true" />
                    </conditions>
                    <action type="Redirect" url="12.0/{R:1}" />
                </rule>

            </rules>
        </rewrite>
    </system.webServer>
</configuration>
