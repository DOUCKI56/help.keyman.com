<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <!-- Desktop engine content hasn't changed between 10.0 -> 12.0 -->
                <rule name="Redirect 11.0/ and 12.0/ to 10.0/">
                    <match url="(11|12)\.0(.*)" />
                    <action type="Redirect" url="10.0{R:2}" />
                </rule>

                <rule name="Redirect unversioned paths to 10.0/ path">
                    <match url="^(.*)" />
                    <conditions>
                        <add input="{URL}" pattern="(9|10)\.0(\/)?" negate="true" />
                    </conditions>
                    <action type="Redirect" url="10.0/{R:1}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
