  <informaltable frame="none"><tgroup colsep="0" rowsep="0" role="RHC.rml10_10__Tavultesoft\table" cols="3"><colspec colnum="1" colname="col1"/><colspec char="RHC.rml10_11\col" colnum="2" colname="col2"/><colspec char="RHC.rml10_12\col" colnum="3" colname="col3"/><tbody><row role="RHC.rml10_13\tr">
      <entry role="RHC.rml10_14\td">
