<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <!-- Desktop engine content hasn't changed between 11.0 and 13.0 -> 11.0 -->
                <rule name="Redirect 13.0, 12.0/ to 10.0/">
                    <match url="(12|13)\.0(.*)" />
                    <action type="Redirect" url="current-version{R:2}" />
                </rule>

                <rule name="Redirect unversioned paths to 11.0/ path">
                    <match url="^(.*)" />
                    <conditions>
                        <add input="{URL}" pattern="(current-version|11\.0)(\/)?" negate="true" />
                    </conditions>
                    <action type="Redirect" url="current-version/{R:1}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
